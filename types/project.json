{
  "name": "types",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "types/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "types/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/utils",
        "tsConfig": "types/tsconfig.lib.json",
        "project": "types/package.json",
        "entryFile": "types/src/index.ts",
        "external": [
          "react",
          "react-dom",
          "react/jsx-runtime"
        ],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "types/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    }
  }
}
